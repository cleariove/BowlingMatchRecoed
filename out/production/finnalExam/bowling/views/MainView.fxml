            <Font size="39.0" />
         </font>
      </Button>
      <Button fx:id="BtPenta" layoutX="407.0" layoutY="364.0" mnemonicParsing="false" onAction="#goToPentaMatch" prefHeight="86.0" prefWidth="173.0" text="五人赛">
         <font>
            <Font size="39.0" />
         </font>
      </Button>
      <Button fx:id="FindScore" layoutX="279.0" layoutY="533.0" mnemonicParsing="false" onAction="#findScore" text="查询成绩" />

      <Button fx:id="BtFinal" layoutX="407.0" layoutY="447.0" mnemonicParsing="false" onAction="#goToFinalMatch" prefHeight="86.0" prefWidth="173.0" text="总决赛">
         <font>
            <Font size="37.0" />
         </font></Button>

   </children>

   <stylesheets>
      <URL value="@style.css" />
   </stylesheets>

