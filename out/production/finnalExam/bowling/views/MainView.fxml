            <Font size="39.0" />
         </font>
      </Button>
      <Button fx:id="BtClassic" layoutX="192.0" layoutY="447.0" mnemonicParsing="false" onAction="#goToClassicMatch" prefHeight="86.0" prefWidth="173.0" text="精英赛" AnchorPane.bottomAnchor="447.0" AnchorPane.rightAnchor="365.0">
