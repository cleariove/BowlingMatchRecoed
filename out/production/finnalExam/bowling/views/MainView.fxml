<AnchorPane prefHeight="980.0" prefWidth="730.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="bowling.controller.MainViewController">
   <children>
      <ImageView fx:id="image" fitHeight="980.0" fitWidth="730.0">
         <image>

            <Image url="bowling/images/8.jpg" />
         </image>

      </ImageView>
      <Button fx:id="BtSingle" layoutX="192.0" layoutY="280.0" mnemonicParsing="false" onAction="#goToSingleMatch" prefHeight="86.0" prefWidth="173.0" text="单人赛" AnchorPane.leftAnchor="192.0" AnchorPane.topAnchor="280.0">
         <font>
            <Font size="39.0" />
         </font></Button>
      <Button fx:id="BtDouble" contentDisplay="TOP" layoutX="407.0" layoutY="280.0" mnemonicParsing="false" onAction="#goToDoubleMatch" prefHeight="86.0" prefWidth="173.0" text="双人赛" AnchorPane.bottomAnchor="614.0" AnchorPane.leftAnchor="407.0">
         <font>
            <Font size="39.0" />
         </font></Button>
      <Button fx:id="BtTriple" layoutX="192.0" layoutY="364.0" mnemonicParsing="false" onAction="#goToTripleMatch" prefHeight="86.0" prefWidth="173.0" text="三人赛" AnchorPane.rightAnchor="365.0" AnchorPane.topAnchor="364.0">
